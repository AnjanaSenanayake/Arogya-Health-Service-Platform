{
  "name": "Arogya-Backend",
  "version": "1.0.0",
  "description": "Node.js Restful CRUD API with Node.js, Express and MySQL",
  "main": "server.js",
  "scripts": {
    "start": "concurrently --kill-others-on-fail \"npm run server\" \"npm run client\"",
    "client": "cd frontend && npm start",
    "server": "cd backend && nodemon server.js"
  },
  "keywords": [
    "nodejs",
    "express",
    "mysql",
    "restapi"
  ],
  "author": "Anjana Senanayake",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "express": "^4.17.1",
    "mysql": "^2.18.1",
    "uuid": "^7.0.3"
  },
  "devDependencies": {
    "concurrently": "^4.0.1"
  }
}
